{
  "terminal.integrated.fontFamily": "Cascadia Mono",
  "terminal.integrated.fontSize": 14,
  "redhat.telemetry.enabled": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line",
          "constant",
          "constant.character",
          "constant.character.escape",
          "constant.numeric",
          "constant.numeric.integer",
          "constant.numeric.float",
          "constant.numeric.hex",
          "constant.numeric.octal",
          "constant.other",
          "constant.regexp",
          "constant.rgb-value",
          "emphasis",
          "entity",
          "entity.name",
          "entity.name.class",
          "entity.name.function",
          "entity.name.method",
          "entity.name.section",
          "entity.name.selector",
          "entity.name.tag",
          "entity.name.type",
          "entity.other",
          "entity.other.attribute-name",
          "entity.other.inherited-class",
          "invalid",
          "invalid.deprecated",
          "invalid.illegal",
          "keyword",
          "keyword.control",
          "keyword.operator",
          "keyword.operator.new",
          "keyword.operator.assignment",
          "keyword.operator.arithmetic",
          "keyword.operator.logical",
          "keyword.other",
          "markup",
          "markup.bold",
          "markup.changed",
          "markup.deleted",
          "markup.heading",
          "markup.inline.raw",
          "markup.inserted",
          "markup.italic",
          "markup.list",
          "markup.list.numbered",
          "markup.list.unnumbered",
          "markup.other",
          "markup.quote",
          "markup.raw",
          "markup.underline",
          "markup.underline.link",
          "meta",
          "meta.block",
          "meta.cast",
          "meta.class",
          "meta.function",
          "meta.function-call",
          "meta.preprocessor",
          "meta.return-type",
          "meta.selector",
          "meta.tag",
          "meta.type.annotation",
          "meta.type",
          "punctuation.definition.string.begin",
          "punctuation.definition.string.end",
          "punctuation.separator",
          "punctuation.separator.continuation",
          "punctuation.terminator",
          "storage",
          "storage.modifier",
          "storage.type",
          "string",
          "string.interpolated",
          "string.other",
          "string.quoted",
          "string.quoted.double",
          "string.quoted.other",
          "string.quoted.single",
          "string.quoted.triple",
          "string.regexp",
          "string.unquoted",
          "strong",
          "support",
          "support.class",
          "support.constant",
          "support.function",
          "support.other",
          "support.type",
          "support.type.property-name",
          "support.variable",
          "variable",
          "variable.language",
          "variable.name",
          "variable.other",
          "variable.other.readwrite",
          "variable.parameter"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },

  "workbench.colorCustomizations": {
    "[Material Theme Darker]": {
      "activityBar.border": "#303030",
      "activityBar.activeBorder": "#00ddff",
      "tab.activeBorder": "#fff",
      "activityBar.activeBackground": "#1638f5",
      "statusBarItem.remoteBackground": "#1e1e1e",
      "statusBarItem.remoteForeground": "#b9b9b9",
      "editorCursor.background": "#000",
      "menu.border": "#313131",
      "menu.separatorBackground": "#313131",
      "editorCursor.foreground": "#fff",
      "editorSuggestWidget.background": "#121212",
      "editorSuggestWidget.selectedBackground": "#000",
      "editorSuggestWidget.highlightForeground": "#02ff17",
      "editorSuggestWidget.focusHighlightForeground": "#c57ff3",
      "editorSuggestWidget.foreground": "#fff",
      "editorSuggestWidget.border": "#292929",
      "scrollbar.shadow": "#000000",
      "statusBar.border": "#303030",
      "sideBar.border": "#303030",
      "tab.hoverForeground": "#fff",
      "tab.hoverBorder": "#02ff17",
      "titleBar.border": "#303030",
      "statusBar.foreground": "#a09e9e",
      "editorGroupHeader.tabsBorder": "#303030",
      "editorGroup.border": "#303030",
      "panel.border": "#303030"
    },
    "[Default Dark Modern]": {
      "scrollbar.shadow": "#000",
      "editor.lineHighlightBackground": "#212121",
      "editor.lineHighlightBorder": "#212121",
      "editorCursor.background": "#000",
      "editorCursor.foreground": "#fff",
      "activityBar.background": "#080808",
      "editor.background": "#090909",
      "sideBar.background": "#101010",
      "statusBar.background": "#090909",
      "tab.activeBorder": "#fff",
      "tab.activeBorderTop": "#2222",
      "tab.hoverBorder": "#00ff0d",
      "menu.background": "#101010",
      "tab.border": "#2222",
      "tab.hoverBackground": "#2222",
      "tab.hoverForeground": "#fff",
      "tab.activeBackground": "#2222",
      "tab.inactiveBackground": "#2222",
      "editorSuggestWidget.background": "#121212",
      "editorSuggestWidget.selectedBackground": "#000",
      "editorSuggestWidget.highlightForeground": "#02ff17",
      "editorSuggestWidget.focusHighlightForeground": "#c57ff3",
      "editorSuggestWidget.foreground": "#fff",
      "editorSuggestWidget.border": "#3c3c3c"
    },
    "[Monokai]": {
      "scrollbar.shadow": "#000",
      "editor.lineHighlightBackground": "#212121",
      "editor.lineHighlightBorder": "#212121",
      "editorCursor.background": "#000",
      "editorCursor.foreground": "#fff",
      "activityBar.background": "#080808",
      "editor.background": "#090909",
      "sideBar.background": "#101010",
      "statusBar.background": "#090909",
      "tab.activeBorder": "#fff",
      "tab.activeBorderTop": "#2222",
      "tab.hoverBorder": "#00ff0d",
      "titleBar.activeBackground": "#101010",
      "menu.background": "#101010",
      "menu.selectionBorder": "#00c3ff",
      "menu.border": "#212121",
      "menu.selectionBackground": "#1e1e1e",
      "tab.border": "#2222",
      "tab.hoverBackground": "#2222",
      "tab.hoverForeground": "#fff",
      "tab.activeBackground": "#2222",
      "tab.inactiveBackground": "#2222",
      "editorSuggestWidget.background": "#121212",
      "editorSuggestWidget.selectedBackground": "#000",
      "editorSuggestWidget.highlightForeground": "#02ff17",
      "editorSuggestWidget.focusHighlightForeground": "#c57ff3",
      "editorSuggestWidget.foreground": "#fff",
      "editorSuggestWidget.border": "#3c3c3c"
    }
  },
  "explorer.confirmDelete": false,
  "cmake.showOptionsMovedNotification": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontLigatures": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.guides.bracketPairs": true,
  "workbench.startupEditor": "none",
  "cmake.configureOnOpen": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.stickyScroll.enabled": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "window.titleBarStyle": "custom",
  "window.customMenuBarAltFocus": true,
  "debug.terminal.clearBeforeReusing": true,
  "editor.fontWeight": "normal",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "vscode-icons",
  "security.workspace.trust.untrustedFiles": "open",
  "git.openRepositoryInParentFolders": "never",
  "editor.fontFamily": "Cascadia Mono",
  "workbench.settings.applyToAllProfiles": [
    "cmake.buildTask"
  ]
}
